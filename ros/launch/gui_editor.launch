<?xml version="1.0"?>
<launch>

    <!-- rviz for visualisation -->
    <node pkg="rviz" type="rviz" name="rviz" args="-d $(find waltzing_robot)/ros/config/gui_editor.rviz" />

    <node pkg="tf" type="static_transform_publisher" name="base_pub" args="0 0 0 0 0 0 /world /robot 30"/>
    <!-- gui editor node -->
    <node pkg="waltzing_robot" type="gui_editor" name="gui_editor" output="screen" />
</launch>
